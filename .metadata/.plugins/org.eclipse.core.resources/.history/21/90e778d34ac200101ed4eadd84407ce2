

public class ShopingTest {
    public static void main(String[] args) {
     	Customer c1 = new Customer("Anshu Tripathi", 5);
        // Create products
        Product p1 = new Electronics("Laptop", 60000.0);
        Product p2 = new Clothing("Shirt", 1500.0);
        Product p3 = new Groceries("Rice (10kg)", 800.0);
        Product p4 = new Electronics("Moblie", 20000.0);

        // Create customer and add products
   
        c1.addProduct(p1);
        c1.addProduct(p2);
        c1.addProduct(p3);
        c1.addProduct(p4);

        // Show final bill
        c1.showBill();
    }
}
